  <animations/>
  <costumes>
    <costume name="arrow2" defaultZOrder="0">
      <pose name="default" pose="arrow2"/>
    </costume>
    <costume name="arrowCurved" defaultZOrder="0">
      <pose name="default" pose="arrowCurved"/>
    </costume>
    <costume name="arrowCurved2" defaultZOrder="0">
      <pose name="default" pose="arrowCurved2"/>
    </costume>
    <costume name="arrowKeys" defaultZOrder="0">
      <pose name="default" pose="arrowKeys"/>
    </costume>
    <costume name="baloon" defaultZOrder="0" role="balloon.py">
      <pose name="default" pose="baloon"/>
    </costume>
    <costume name="chair" defaultZOrder="0" role="chair.py">
      <pose name="default" pose="chair"/>
    </costume>
    <costume name="coin" defaultZOrder="0" role="collectable.py" properties="roundProperties.py">
      <pose name="default" pose="coin"/>
      <properties roundedNE="true" roundedSE="true" roundedSW="true" roundedNW="true"/>
    </costume>
    <costume name="gate" defaultZOrder="0" role="gate.py">
      <pose name="default" pose="gateClosed"/>
      <pose name="open" pose="gateOpen"/>
    </costume>
    <costume name="keyEscape" defaultZOrder="0">
      <pose name="default" pose="keyEscape"/>
    </costume>
    <costume name="keyF5" defaultZOrder="0">
      <pose name="default" pose="keyF5"/>
    </costume>
    <costume name="player" defaultZOrder="0" role="player.py">
      <pose name="default" pose="alien1a"/>
    </costume>
    <costume name="portcullis" defaultZOrder="0" role="portcullis.py">
      <pose name="close" pose="wall"/>
      <pose name="default" pose="portcullis"/>
    </costume>
    <costume name="rock" defaultZOrder="0" role="rock.py">
      <pose name="default" pose="rock"/>
    </costume>
    <costume name="soil" defaultZOrder="0" role="soft.py">
      <pose name="default" pose="soil"/>
    </costume>
    <costume name="stamp" defaultZOrder="0" role="collectable.py" properties="roundProperties.py">
      <pose name="default" pose="stamp"/>
      <properties roundedNE="false" roundedSE="false" roundedSW="false" roundedNW="false"/>
    </costume>
    <costume name="table" defaultZOrder="0" role="table.py">
      <pose name="default" pose="table"/>
    </costume>
    <costume name="wall" defaultZOrder="0" role="wall.py" properties="roundProperties.py">
      <pose name="default" pose="wall"/>
      <properties roundedNE="false" roundedSE="false" roundedSW="false" roundedNW="false"/>
    </costume>
    <costume name="wallNE" defaultZOrder="0" role="wall.py" properties="roundProperties.py">
      <pose name="default" pose="wallNE"/>
      <properties roundedNE="true" roundedSE="false" roundedSW="false" roundedNW="false"/>
    </costume>
    <costume name="wallNW" defaultZOrder="0" role="wall.py" properties="roundProperties.py">
      <pose name="default" pose="wallNW"/>
      <properties roundedNE="false" roundedSE="false" roundedSW="false" roundedNW="true"/>
    </costume>
    <costume name="wallSE" defaultZOrder="0" role="wall.py" properties="roundProperties.py">
      <pose name="default" pose="wallSE"/>
      <properties roundedNE="false" roundedSE="true" roundedSW="false" roundedNW="false"/>
    </costume>
    <costume name="wallSW" defaultZOrder="0" role="wall.py" properties="roundProperties.py">
      <pose name="default" pose="wallSW"/>
      <properties roundedNE="false" roundedSE="false" roundedSW="true" roundedNW="false"/>
    </costume>
    <costume name="warp" defaultZOrder="0" role="warp.py">
      <pose name="close" pose="gateClosed"/>
      <pose name="default" pose="gateOpen"/>
    </costume>
  </costumes>
  <scenes>
    <scene name="glass"/>
    <scene name="level-rocksAndCoins"/>
    <scene name="level-startSimple"/>
