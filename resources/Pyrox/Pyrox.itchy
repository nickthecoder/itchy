      </parallel>
    </animation>
    <animation name="player-hit">
      <sequence sequence="true" loops="1">
        <parallel sequence="false" loops="1">
          <move dx="0.0" dy="200.0" ticks="100" ease="easeIn"/>
          <alpha target="128.0" ticks="40" ease="default"/>
          <scale target="0.75" ticks="100" ease="easeOut"/>
          <sequence sequence="true" loops="1">
            <move dx="-30.0" dy="0.0" ticks="10" ease="easeOut"/>
            <move dx="30.0" dy="0.0" ticks="10" ease="easeIn"/>
            <move dx="20.0" dy="0.0" ticks="10" ease="easeOut"/>
            <move dx="-20.0" dy="0.0" ticks="10" ease="easeIn"/>
            <move dx="-10.0" dy="0.0" ticks="10" ease="easeOut"/>
            <move dx="10.0" dy="0.0" ticks="10" ease="easeIn"/>
            <move dx="5.0" dy="0.0" ticks="10" ease="easeOut"/>
            <move dx="-5.0" dy="0.0" ticks="10" ease="easeIn"/>
          </sequence>
          <sequence sequence="true" loops="7">
            <frames pingPong="false">
              <frame pose="robot-R3" delay="4" dx="0.0" dy="0.0"/>
              <frame pose="robot-L3" delay="4" dx="0.0" dy="0.0"/>
              <frame pose="robot-D1" delay="4" dx="0.0" dy="0.0"/>
            </frames>
          </sequence>
        </parallel>
        <parallel sequence="false" loops="1">
          <scale target="3.0" ticks="100" ease="easeOutBack"/>
          <alpha target="0.0" ticks="100" ease="easeOut"/>
          <sequence sequence="true" loops="10">
            <frames pingPong="false">
              <frame pose="robot-R2" delay="6" dx="0.0" dy="0.0"/>
              <frame pose="robot-L2" delay="6" dx="0.0" dy="0.0"/>
              <frame pose="robot-D1" delay="6" dx="0.0" dy="0.0"/>
            </frames>
          </sequence>
        </parallel>
      </sequence>
    </animation>
