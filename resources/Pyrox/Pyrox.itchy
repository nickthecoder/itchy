          <alpha target="0.0" ticks="30" ease="easeInOut"/>
          <scale target="0.02" ticks="15" ease="easeInOut"/>
          <move dx="0.0" dy="90.0" ticks="30" ease="easeOutBack"/>
        </parallel>
      </sequence>
    </animation>
