    </animation>
    <animation name="bee-escapeE">
      <sequence sequence="true" loops="1" finishedMessage="unpause">
        <parallel sequence="false" loops="1">
          <move dx="-60.0" dy="0.0" ticks="1" ease="linear"/>
          <alpha target="128.0" ticks="1" ease="linear"/>
          <scale target="0.1" ticks="1" ease="linear"/>
          <frames pingPong="false">
            <frame pose="bee-E1" delay="1" dx="0.0" dy="0.0"/>
          </frames>
        </parallel>
        <parallel sequence="false" loops="1">
          <alpha target="255.0" ticks="15" ease="easeInOut"/>
          <scale target="1.0" ticks="30" ease="linear"/>
          <move dx="60.0" dy="0.0" ticks="30" ease="easeInOut"/>
        </parallel>
      </sequence>
    </animation>
    <animation name="bee-escapeN">
