        <delay ticks="30"/>
        <parallel sequence="false" loops="1">
          <scale target="1.0" ticks="15" ease="linear"/>
        </parallel>
      </sequence>
    </animation>
    <animation name="collectable-collected">
      <parallel sequence="false" loops="1">
        <alpha target="0.0" ticks="80" ease="easeIn"/>
