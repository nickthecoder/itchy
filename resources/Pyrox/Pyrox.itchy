        <parallel sequence="false" loops="1">
          <scale target="1.0" ticks="30" ease="easeInOut"/>
          <alpha target="255.0" ticks="15" ease="easeInOut"/>
          <move dx="-60.0" dy="0.0" ticks="30" ease="easeInOut"/>
        </parallel>
      </sequence>
