  <animations>
    <animation name="alien1">
      <compound sequence="true" loops="1">
        <frames pingPong="false">
          <frame pose="alien1a" delay="1"/>
          <frame pose="alien1b" delay="1"/>
        </frames>
      </compound>
    </animation>
    <animation name="alien2">
      <compound sequence="true" loops="1">
        <frames pingPong="false">
          <frame pose="alien2a" delay="1"/>
          <frame pose="alien2b" delay="1"/>
        </frames>
      </compound>
    </animation>
    <animation name="alien3">
      <compound sequence="true" loops="1">
        <frames pingPong="false">
          <frame pose="alien3a" delay="1"/>
          <frame pose="alien3b" delay="1"/>
        </frames>
      </compound>
    </animation>
    <animation name="alienYell">
      <compound sequence="false" loops="1">
        <alpha target="0.0" ticks="180" ease="linear"/>
        <turn turn="40.0" ticks="180" ease="linear"/>
      </compound>
    </animation>
    <animation name="bomb">
      <compound sequence="true" loops="0">
        <frames pingPong="false">
          <frame pose="bomb1" delay="10"/>
          <frame pose="bomb2" delay="10"/>
        </frames>
      </compound>
    </animation>
    <animation name="deathSpiral">
      <compound sequence="false" loops="1">
        <alpha target="0.0" ticks="40" ease="linear"/>
        <move dx="0.0" dy="100.0" ticks="180" ease="easeOut"/>
      </compound>
    </animation>
    <animation name="fadeIn">
      <compound sequence="true" loops="1">
        <alpha target="0.0" ticks="1" ease="linear"/>
        <alpha target="255.0" ticks="40" ease="linear"/>
      </compound>
    </animation>
    <animation name="fadeOut">
      <compound sequence="true" loops="1">
        <alpha target="0.0" ticks="40" ease="linear"/>
      </compound>
    </animation>
    <animation name="grow">
      <compound sequence="true" loops="1">
        <scale target="0.1" ticks="1" ease="linear"/>
        <scale target="1.0" ticks="100" ease="linear"/>
      </compound>
    </animation>
    <animation name="pacman">
      <compound sequence="false" loops="1">
        <compound sequence="true" loops="0">
          <frames pingPong="false">
            <frame pose="pacman1" delay="3"/>
            <frame pose="pacman2" delay="3"/>
            <frame pose="pacman3" delay="3"/>
            <frame pose="pacman4" delay="3"/>
            <frame pose="pacman5" delay="3"/>
          </frames>
        </compound>
        <compound sequence="true" loops="1">
          <forwards forwards="400.0" sideways="0.0" ticks="200" ease="linear"/>
          <compound sequence="false" loops="1">
            <forwards forwards="890.0" sideways="0.0" ticks="360" ease="linear"/>
            <turn turn="-360.0" ticks="360" ease="linear"/>
          </compound>
          <forwards forwards="400.0" sideways="0.0" ticks="200" ease="linear"/>
          <turn turn="180.0" ticks="10" ease="linear"/>
          <forwards forwards="400.0" sideways="0.0" ticks="200" ease="linear"/>
          <compound sequence="false" loops="1">
            <forwards forwards="890.0" sideways="0.0" ticks="360" ease="linear"/>
            <turn turn="360.0" ticks="360" ease="linear"/>
          </compound>
          <forwards forwards="400.0" sideways="0.0" ticks="200" ease="linear"/>
        </compound>
      </compound>
    </animation>
    <animation name="toHeaven">
      <compound sequence="false" loops="1">
        <move dx="0.0" dy="60.0" ticks="120" ease="linear"/>
        <alpha target="0.0" ticks="120" ease="linear"/>
      </compound>
    </animation>
  </animations>
