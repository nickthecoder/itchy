      <string name="yell">I'll be back</string>
      <string name="yell">OMG</string>
      <sound name="shot" sound="shatter" fadeOutSeconds="1.0" fadeOnDeath="false" multipleRole="PLAY_BOTH" priority="1"/>
      <font name="speech" font="vera"/>
      <font name="yell" font="vera"/>
      <animation name="birth" animation="fadeIn"/>
      <animation name="death" animation="deathSpiral"/>
